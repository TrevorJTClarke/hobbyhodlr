{
	"name": "account-rules",
	"rules": [
		 {
			 "when": { "closure": "balanceBelowThreshold" },
			 "then": "alertBalanceLow"
		 },
		 {
			 "when": { "closure": "balanceLargeMovement" },
			 "then": "alertBalanceLargeMovement"
		 },
		 {
			 "when": { "closure": "balanceReward" },
			 "then": "alertBalanceReward"
		 }
	]
}
